package guessing;

import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Game game = new Game();
		game.generateNumber();
		
		boolean temp = true;
		while(temp) {
		Scanner sc = new Scanner(System.in);
		int input = sc.nextInt();
		System.out.println(game.humanGuess(input));
		}
		
		
	}

}
